    "paths": {
        "/vhs/getall": {
            "get": {
                "operationId": "e77d5f5d705353a9c9351c0552c5a844",
                "responses": {
                    "200": {
                        "description": "get all vhs",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Vhs"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/vhs/create": {
            "post": {
                "operationId": "882ecec35269a81b0feb1743b438ef0c",
                "requestBody": {
                    "$ref": "#/components/requestBodies/AddVhs"
                },
                "responses": {
                    "201": {
                        "description": "insert vhs",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "description": "response of insering vhs",
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "insert vhs bad parameters",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "description": "response of insering vhs with bad parameters",
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
